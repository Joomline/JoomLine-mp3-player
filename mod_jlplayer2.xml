<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="2.5" client="site" method="upgrade">
    <name>JoomLine mp3 player v2</name>
    <creationDate>2014-01-22</creationDate>
    <author>JoomLine</author>
    <authorEmail>sale@joomline.ru</authorEmail>
    <authorUrl>http://www.joomline.net</authorUrl>
    <copyright>Copyright 2010 JoomLine.net All rights reserved!</copyright>
    <license>http://joomline.net/licenzija-joomline.html Licenses JoomLine</license>
    <version>3.6.1</version>
    <description>
        <![CDATA[
			<style type="text/css">
				.readonly.mod-desc{margin:12px 0;padding:20px 0;border-top:1px #eee solid;border-bottom:1px #eee solid;overflow:hidden;}
				#module-sliders{font:14px/18px "Trebuchet MS",Arial,Helvetica,sans-serif;color:#6D6D6D;}
				#module-sliders h3{padding:10px;background-color:#E7E7E7;font-size:14px;}
				#module-sliders h3 a{color:#000;}
				#module-sliders .pane-toggler span,#module-sliders .pane-toggler-down span{background:url('data:image/png;base64,') no-repeat 0 -24px;padding-left:17px;}
				#module-sliders .pane-toggler-down span{background-position:0 1px;}
				#module-sliders .adminformlist li:after{clear:both;content:' ';display:block;font-size:0;line-height:0;visibility:hidden;width:0;height:0;}#module-sliders .adminformlist li{display:inline-block;}* html #module-sliders .adminformlist li{height:1%;}
				#module-sliders .adminformlist li{display:block;padding:5px 0 10px;margin-bottom:5px;border-bottom:1px #eee solid;}s
				#module-sliders label{min-height:30px;margin:0;line-height:30px;color:#343434;}
				#module-sliders .jform_params_plvisible label{color:#333;}
				#module-sliders input[type=text],#module-sliders textarea,#module-sliders select{height:30px;width:200px;margin:0;line-height:30px;padding:0 5px;color:#666;border:1px #E1E1E1 solid;font-size:15px;background-color:#D5EEFF;}
				#module-sliders select{height:34px;line-height:34px;width:213px;padding:5px;}
				#module-sliders textarea{min-height:150px;width:98%;padding:5px;}
				#module-sliders .radio{margin:0;}
				#module-sliders input[type=radio]{position:relative;top:3px;}
				#module-sliders #jform_params_playlist-lbl{float:none;display:block;}
				#module-sliders #jform_params_playlist{height:260px;overflow:hidden;width:100%;padding:0;border:1px #eee solid;-moz-box-sizing:border-box;box-sizing: border-box;}
				#module-sliders #jform_params_playlist ul{height:222px;overflow:auto;margin-top:3px;border-bottom:1px #eee solid;}
				#module-sliders #jform_params_playlist li{height:35px;line-height:35px;margin:0 3px;padding:0 10px;background:#D5EEFF;border:0 none;}
				#module-sliders #jform_params_playlist li:nth-child(2n){background:#fff;}
				#module-sliders #jform_params_playlist label{height:35px;max-width:100%;width:100%;line-height:35px;display:block;float:none;margin:0;padding:0;cursor:pointer;font-size:12px;}
				#module-sliders #jform_params_playlist input{margin:11px 8px 0 0;}
				#add_playlist{height:30px;display:inline-block;margin:0 0 0 10px;line-height:30px;padding:0 10px;border:1px #ccc solid;text-decoration:none;font-weight:bold;color:#6D6D6D;}
				#add_playlist:hover{background:#D5EEFF;}
				.sellinks{float:right;margin:6px 15px 0 0;}
				.select_check{display:inline-block;color:#0B55C4;cursor:pointer;}	
				#jform_params_playlist .error{text-align:center;margin-top:108px;font-size:18px;color:red;}
			</style>
			<script type="text/javascript">
			function checkAll(formname, checktoggle) {
				var checkboxes = new Array();
				checkboxes = document[formname].getElementsByTagName('input');
				for (var i = 0; i < checkboxes.length; i++) {
					if (checkboxes[i].type === 'checkbox') {
						checkboxes[i].checked = checktoggle;
					}
				}
			}
			</script>
			<div id="admin_info">
            <p>Version - New Version</p>
            <p>Show mp3 player that scan selected directory for mp3 files</p>
            <p><a target="_blank" href="http://joomline.net/licenzija-joomline.html">Licenses JoomLine</a></p>
            <p><a target="_blank" href="mailto:sale@joomline.ru">Support</a></p>
            <p><a target="_blank" href="http://www.joomline.net"><img src="data:image/png;base64,"></a></p>
			</div>
			]]>
    </description>

    <files>
        <folder>assets</folder>
        <folder>language</folder>
        <folder>lib</folder>
        <folder>models</folder>
        <folder>skins</folder>
        <folder>tmpl</folder>
        <filename>helper.php</filename>
        <filename>index.html</filename>
        <filename module="mod_jlplayer2">mod_jlplayer2.php</filename>
        <filename>mod_jlplayer2.xml</filename>
        <filename>standalone.php</filename>
    </files>

    <languages folder="languages">
        <language tag="en-GB">language/en-GB/en-GB.mod_jlplayer2.ini</language>
        <language tag="ru-RU">language/ru-RU/ru-RU.mod_jlplayer2.ini</language>
    </languages>


    <config>
        <fields name="params">
            <fieldset name="basic" addfieldpath="/modules/mod_jlplayer2/models/fields">
                <field name="directory" type="directory" directory="music" label="MOD_JLP_DIRECTORY"
                       description="MOD_JLP_DIRECTORY_DESC" default="music" button="MOD_JLP_BTN_SAVE_DIR"/>
                <field name="playlist" type="playlist" directory="directory" label="MOD_JLP_BTN_PLAYLIST"
                       description="MOD_JLP_BTN_PLAYLIST_DESC"/>
                <field name="customlist" type="textarea" label="MOD_JLP_BTN_CUSTOM_PLAYLIST"
                       description="MOD_JLP_BTN_CUSTOM_PLAYLIST_DESC"/>
            </fieldset>

            <fieldset name="tools" label="MOD_JLP_FLSET_TOOLS">

                <field name="width" type="text" default="420px" label="MOD_JLP_PL_WIDTH"
                       description="MOD_JLP_PL_WIDTH_DESC"/>

                <field name="server_utf8" type="radio" default="0" label="MOD_JLP_PL_UTF8"
                       description="MOD_JLP_PL_UTF8_DESC">
                    <option value="0">MOD_JLP_PL_NO</option>
                    <option value="1">MOD_JLP_PL_YES</option>
                </field>

                <field name="scroll_track_name" type="radio" default="1" label="MOD_JLP_PL_SCROLL_TRACK_NAME"
                       description="MOD_JLP_PL_SCROLL_TRACK_NAME_DESC">
                    <option value="0">No</option>
                    <option value="1">Yes</option>
                </field>

                <field name="shuffle" type="radio" default="1" label="MOD_JLP_PL_SHUFFLE"
                       description="MOD_JLP_PL_SHUFFLE_DESC">
                    <option value="0">No</option>
                    <option value="1">Yes</option>
                </field>

                <field name="plvisible" type="radio" default="2" label="MOD_JLP_PL_STARTUP_STATUS"
                       description="MOD_JLP_PL_STARTUP_STATUS_DESC">
                    <option value="0">MOD_JLP_PL_FOLDED</option>
                    <option value="1">MOD_JLP_PL_UNFOLDED</option>
                    <option value="2">MOD_JLP_PL_DEFAULT</option>
                </field>

                <field name="autoplay" type="radio" default="0" label="MOD_JLP_PL_AUTOPLAY"
                       description="MOD_JLP_PL_AUTOPLAY_DESC">
                    <option value="0">MOD_JLP_PL_NO</option>
                    <option value="1">MOD_JLP_PL_YES</option>
                </field>

                <field name="onebutton" type="radio" default="0" label="MOD_JLP_PL_ONLY_BUTTON"
                       description="MOD_JLP_PL_ONLY_BUTTON_DESC">
                    <option value="0">MOD_JLP_PL_NO</option>
                    <option value="1">MOD_JLP_PL_YES</option>
                </field>

                <field name="enablejquery" type="radio" default="1" label="MOD_JLP_PL_ENABLE_JQ"
                       description="MOD_JLP_PL_ENABLE_JQ_DESC">
                    <option value="0">MOD_JLP_PL_NO</option>
                    <option value="1">MOD_JLP_PL_YES</option>
                </field>

                <field name="jqnoconflict" type="radio" default="1" label="MOD_JLP_PL_JQNOCONFLICT"
                       description="MOD_JLP_PL_JQNOCONFLICT_DESC">
                    <option value="0">MOD_JLP_PL_NO</option>
                    <option value="1">MOD_JLP_PL_YES</option>
                </field>

                <field name="skin" default="skin-light" directory="modules/mod_jlplayer2/skins" type="skinlist"
                       label="Select Skin" description="Select Skin"/>

            </fieldset>

            <fieldset name="advanced">
                <field name="moduleclass_sfx" type="text" label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
                       description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC"/>
                <field name="owncache" type="list" default="1"
                       label="COM_MODULES_FIELD_CACHING_LABEL" description="COM_MODULES_FIELD_CACHING_DESC">
                    <option value="1">JGLOBAL_USE_GLOBAL</option>
                    <option value="0">COM_MODULES_FIELD_VALUE_NOCACHING
                    </option>
                </field>
                <field name="cache_time" type="text" default="900" label="COM_MODULES_FIELD_CACHE_TIME_LABEL"
                       description="COM_MODULES_FIELD_CACHE_TIME_DESC"/>
            </fieldset>
        </fields>
    </config>
</extension>
